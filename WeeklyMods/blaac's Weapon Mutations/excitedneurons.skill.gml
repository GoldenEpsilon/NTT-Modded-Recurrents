with BouncerBullet{
	if ("bounce2" not in self){
		bounce2 = -2
		image_blend = merge_color(image_blend, c_white, 0.3)
		}
	if bounce = 1{
		bounce = 0
		bounce2 += 1
		damage += 1
		speed += 1
		image_blend = merge_color(image_blend, c_orange, 0.3)
		instance_create(x,y,CaveSparkle)
		}
	if bounce2 > 3
		instance_destroy()
	}

with Bullet1{
	with instance_create(x,y,BouncerBullet){
		direction = other.direction
		image_angle = random(360)
		speed = 11
		team = other.team
		damage = other.damage
		creator = other.creator
		bounce2 = 0
		if instance_exists(creator) && (creator.wep = 81||creator.wep = 26||creator.wep = 65){
			sprite_index = global.idpdbouncer
			}
		}
	instance_change(Wind,0)
	instance_destroy()
	}
	
with HeavyBullet{
	with instance_create(x,y,BouncerBullet){
		direction = other.direction
		image_angle = random(360)
		speed = 11
		team = other.team
		damage = other.damage
		creator = other.creator
		bounce2 = 0
