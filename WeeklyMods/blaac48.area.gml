#define largelad_create(_x,_y)
with instance_create(_x,_y,CustomEnemy){
    name = "friendliness bug"
    boss = 1
    size = 20
    team = 1
    target = null
    targetvisible = 0
    maxhealth = 500+(100*(instance_number(Player)-1))
    my_health = maxhealth
    spr_idle = global.bigflyidle
    spr_walk = global.bigflywalk
    spr_chrg = global.bigflycharge
    spr_hurt = global.bigflyhurt
    spr_dead = global.bigflyhurt
    snd_hurt = sndFireballerHurt
    snd_dead = sndFireballerDead
    mask_index = mskFrogQueen
    spr_shadow = shd64B
    raddrog = 200
    depth = -2
    right = 1
    hitid = [spr_idle,name]
    on_step = largelad_step
    on_hurt = largelad_hurt
    on_death = largelad_dead
    maxspeed = 2
    canmelee = 1
    meleedamage = 3
    corpse = 0
    chargedir = direction
    walk = 0
    alarm0 = 300    //attack
    alarm1 = 0 //charging
    alarm2 = 0 //tell
    
    return id;
}
#define largelad_step
if(nexthurt > current_frame){
		if(sprite_index != spr_hurt) {
			image_index = 0;
			sprite_index = spr_hurt;}
		}
	else{
		if(speed > friction) 
			sprite_index = spr_walk;
		else 
			sprite_index = spr_idle;
		}
if(instance_exists(Player))
	target = instance_nearest(x,y,Player);
	else
	target = 0;
	
if(instance_exists(target) && collision_line(x,y,target.x,target.y,Wall,0,0) < 0){
	targetvisible = 1;
	if target.x > x
		right = 1
	else
		right = -1
    }
	else 
	targetvisible = 0;

image_xscale = right

if sprite_index = spr_walk
    image_speed = 0.8
else
    image_speed = 0.4

if walk > 0{
	walk -= current_time_scale
	motion_add_ct(direction, 0.8)
	}
	
speed = max(min(speed,maxspeed+(alarm1)),0)

if !walk and !alarm1 and !alarm2 and random(25)<1{
    if targetvisible
        direction = point_direction(x,y,target.x,target.y)
    else
        direction = random(360)
    walk += 30
}



alarm0=max(alarm0-current_time_scale-targetvisible,0)

if alarm2>0{
    sprite_index = spr_chrg
    speed *= 0.9
    image_index = 1
    alarm2 -= 1
}
if !alarm2 and sprite_index = spr_chrg{
    alarm1 = 40
    sprite_index = spr_walk
    pickup_drop(75,0,1)
    sound_play_pitchvol(sndSuperFireballerHurt, 0.7 + random(0.1), 2);
}

if !alarm0 and target!=0{
    walk = 0
    alarm0 = 200
    sprite_index = spr_chrg
    alarm2 = 20
    chargedir = point_direction(x,y,target.x,target.y)
    sound_play_pitchvol(sndFireballerFire, 0.5 + random(0.1), 2);
}
if alarm1>0{
    alarm1 -= 1
    if distance_to_object(Wall)<16{
        instance_create(x,y,PortalClear)
        alarm1 = max(0,alarm1-1)
    }
	motion_add_ct(chargedir, 1)
	direction = chargedir
}


if random(7)<1
	instance_create(x+random_range(-20,20),y+random_range(-20,0),FireFly)

#define largelad_hurt(_hitdmg, _hitvel, _hitdir)
my_health -= _hitdmg;
if !alarm1
motion_add(_hitdir, _hitvel);
nexthurt = current_frame + 6;

if(point_seen(x, y, -1)){
    sound_play_pitchvol(snd_hurt, 0.5 + random(0.1), 1);
}

sprite_index = spr_hurt;
image_index = 0;
#define largelad_dead
sound_play_pitchvol(snd_dead, 0.5 + random(0.1), 4);
with instance_create(x,y,Corpse){
    image_xscale = other.right
    sprite_index = other.spr_dead
    image_blend = c_gray
    do_portal = 0
    if fork(){
        wait 120
        if instance_exists(self){
            repeat(32) 
            with instance_create(x,y,Dust){
                motion_add(random(360),random(6))
                vspeed-=random(1)
            }
            repeat 5
            with instance_create(x, y , Sapling){
                    hitid = [sprSaplingIdle,"saple of evil"] 
                    team = 1
                    mask_index = mskBandit
                }
            with instance_create(x,y,Rat){
                image_xscale = 2
                image_yscale = 2
                maxhealth = 30
                my_health = maxhealth
                spr_idle = sprFastRatIdle
                spr_walk = sprFastRatWalk
                spr_hurt = sprFastRatHurt
                spr_dead = sprFastRatDead
                hitid = [spr_idle,"rat of justice"] 
            }
            repeat(12)
            with instance_create(x,y,Corpse){
                sprite_index = sprDebris7
                image_speed = 0
                image_index = irandom(image_number)
                motion_add(random(360),random(14))
            }
            repeat(12)
            with instance_create(x,y,Debris){
                sprite_index = sprDebris7
                image_speed = 0
                image_index = irandom(image_number)
                motion_add(random(360),random(14))
            }
            repeat(120)
            with instance_create(x,y,Feather){
                sprite_index = sprChickenFeather
                image_blend = c_purple
                friction = 0.4
                motion_add(random(360),random(4))
            }
        instance_destroy()
        }
        
        exit;
    }
}

#define nothing(_hitdmg, _hitvel, _hitdir)
if(point_seen(x, y, -1)){
    sound_play_pitchvol(snd_hurt, 0.5 + random(0.1), 1);
}
my_health -= _hitdmg;
nexthurt = current_frame + 6;