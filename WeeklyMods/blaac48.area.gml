with instance_nearest(0,0,Floor){
    if fork(){
        while instance_exists(self){
            if random(16)<1 with instance_create(view_xview+random(game_width*2)-game_width/2,view_yview+random(game_height*2)-game_height/2,SnowFlake){
		        sprite_index = sprLeaf
		        image_blend = c_orange
            }
		    with Wall image_blend = c_orange
		    with TopSmall image_blend = c_orange
		    with JungleAssassinHide{
		        if sprite_index != spr_hurt and sprite_index != spr_dead
		            image_blend = c_orange
		        else image_blend = c_white
		    }
		    with JungleAssassin{
		        if sprite_index != spr_hurt and sprite_index != spr_dead
		            image_blend = c_orange
		        else image_blend = c_white
		    }
		    with Player
		    with enemy if random(24)<1 and ("boss" not in self or alarm1 = 0) and !collision_rectangle(view_xview,view_yview,view_xview+game_width,view_yview+game_height,self,0,0){
		        var rand = random(360);
		        var px = Player.x+lengthdir_x(game_width+64,rand)
		        var py = Player.y+lengthdir_y(game_width+64,rand)
		        
		        var rf = instance_nearest(px,py,Floor);
		        var nocrash = 0;
		        while collision_rectangle(view_xview,view_yview,view_xview+game_width,view_yview+game_height,rf,0,0){
		            rand = random(360);
		            px = Player.x+lengthdir_x(game_width+64+nocrash,rand)
		            py = Player.y+lengthdir_y(game_width+64+nocrash,rand)
		            rf = instance_nearest(px,py,Floor);
		            nocrash += 16
		        }
		        x = rf.x+(sprite_get_width(rf.sprite_index)/2)
		        y = rf.y+(sprite_get_width(rf.sprite_index)/2)
		    }
		    
		    //with EnemyBullet2{
		    //    if random(2)<1
		    //        instance_delete(id)
		    //    else with instance_create(x,y,FiredMaggot){
		    //        team = other.team
		    //        speed = other.speed
		    //        direction = other.direction
		    //        image_angle = direction
		    //        with other
		    //        instance_delete(id)
		    //    }
		    //}
		    
		    with BigFlower if random(300)<1
		        with instance_create(x, y , Sapling){
                    hitid = [sprSaplingIdle,"saple of evil"] 
                    team = 1
                    meleedamage = 1
                    canmelee = 1
                    mask_index = mskBandit
                }
            with instances_matching(Sapling,"team",1){
                raddrop = 0
                speed = min(speed,2)
		        if random(32)<1 
		        with instance_create(x,y,ToxicGas){
		            team = 1 
		            friction = 0.1
		        }
		        
		        if sprite_index != spr_hurt
		            image_blend = c_orange
		        else if image_index < 1 image_blend = c_white
            }
		    
		    with Rat{
		        if random(20)<1
		            motion_add(random(360),4)
		            if random(10)<1
		                instance_create(x+random_range(-4,4),y+random_range(-4,4),FireFly)
		    }
		    
		    with instances_matching(Corpse,"sprite_index",sprJungleFlyDead)
		        speed *= 0.9
		    
		    with JungleAssassin if speed = 0 and random(100)<1{
		        with instance_create(x,y,JungleAssassinHide)
                    image_blend = c_orange
                repeat 3
                with instance_create(x+random_range(-3,3),y+random_range(-3,3),Feather)
                    sprite_index = sprLeaf
                instance_delete(id)
		    }
		        
		    with Portal{
		        var p = self;
		        with Wall if distance_to_object(other)<64 and random(40)<1
		            with instance_create(x+random_range(-4,4),y+random_range(-4,4),CaveSparkle){
		                sprite_index = sprLeaf
		                motion_set(point_direction(x,y,p.x,p.y),distance_to_object(p)/10)
		            }
		    }
		    
		    with instances_matching(Cactus,"spr_idle",sprVaultStatue)
		        if sprite_index = spr_idle
		            image_index = 6
		    
		    with instances_matching(instances_matching(Corpse,"sprite_index",sprVaultStatueDead),"dog",null){
		        with instance_create(x,y,EnemyHorror)
		            hitid = [spr_idle,"dog of misfortune"]
		        dog = 1
		    }
		    
		    with instances_matching(FloorExplo,"newleaf",null){
		        repeat(random(6))
		        with instance_create(x+8+random_range(-4,4),y+8+random_range(-4,4),Feather){
		            sprite_index = sprLeaf
		        
		            motion_add(random(360),random(1))
		        }
		        newleaf = 1
		    }
		    
		    with instances_matching(Rat,"spr_idle",sprFastRatIdle)
		        if random(24)<1 
		        with instance_create(x,y,ToxicGas){
		            team = 1 
		            friction = 0.1
		        }
		    //with Player candie = 0
            wait 1
        }
        exit;
    }
}



